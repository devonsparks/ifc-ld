@base <http://ifc-ld.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .


:Container
    a rdfs:Class .

:Package 
    a rdfs:Class ;
    rdfs:subclassOf :Container .

:Object
    a rdfs:Class ;
    rdfs:subclassOf :Container .

:Slot
    a rdfs:Class ;
    rdfs:subclassOf :Container .

:slotof a rdf:Property ;
    rdfs:domain :Slot ;
    rdfs:range :Object .

:slots a rdf:Property ;
    rdfs:domain :Object ;
    rdfs:range :Slot .

:shortcut a rdf:Property ;
    rdfs:domain :Slot .

:objectof a rdf:Property ;
    rdfs:domain :Object ;
    rdfs:range :Package .

:objects a rdf:Property ;
    rdfs:domain :Package ;
    rdfs:range :Object .

:who a rdf:Property ;
    rdfs:subPropertyOf prov:wasAttributedTo .

:when a rdf:Property ;
    rdfs:subPropertyOf prov:generatedAtTime .

:contents a rdf:Property .

:group a rdf:Property ;
    rdfs:subPropertyOf xsd:string .
